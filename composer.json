{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "archilex/filament-toggle-icon-column": "^3.1",
        "bezhansalleh/filament-shield": "^3.2",
        "doctrine/dbal": "^4.0",
        "filament/filament": "^3.2",
        "joaopaulolndev/filament-edit-profile": "^1.0",
        "joaopaulolndev/filament-general-settings": "^1.0",
        "laravel/framework": "^11.9",
        "laravel/tinker": "^2.9",
        "leandrocfe/filament-ptbr-form-fields": "^3.0",
        "marcogermani87/filament-cookie-consent": "^1.0",
        "marjose123/filament-lockscreen": "^2.0",
        "mokhosh/filament-rating": "^1.1",
        "njxqlus/filament-progressbar": "^1.0",
        "z3d0x/filament-logger": "^0.7.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.23",
        "laradumps/laradumps": "^3.1",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "lucascudo/laravel-pt-br-localization": "^2.2",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1",
        "squizlabs/php_codesniffer": "^3.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "generate": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models -N"
        ],
        "ds:check": "@php vendor/bin/laradumps check --dir=app,bootstrap,database,routes,config --text=dump,dd,var_dump",
        "verify": [
            "@ds:check"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@generate"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
